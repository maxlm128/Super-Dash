package main;

public class ExtSpikeNeck extends Entity{
	public ExtSpikeNeck(int pColor,char rotation) {
		color = pColor;
		switch(rotation) {
		case'd':
			content = '>';
			break;
		case'a':
			content = '<';
			break;
		case'w':
			content = '^';
			break;
		case's':
			content = 'v';
			break;
		}
	}
}
